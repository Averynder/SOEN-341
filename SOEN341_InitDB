  CONSTRAINT `fk_lecture_teacher1`
    FOREIGN KEY (`-Instructor`)
    REFERENCES `soen341`.`teacher` (`-Name`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


