-- -----------------------------------------------------
-- Table `soen341`.`account user`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`account user` (
  `-Netname` TEXT NULL DEFAULT NULL,
  `-Password` TEXT NULL DEFAULT NULL)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`teacher`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`teacher` (
  `-Name` TEXT NOT NULL,
  `-Faculty ID` INT(11) NOT NULL,
  `-Course List` VARCHAR(200) NOT NULL,
  `-Course History` TEXT NOT NULL,
  PRIMARY KEY (`-Course List`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`student`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`student` (
  `-Name` TEXT NOT NULL,
  `-Student ID` INT(11) NOT NULL,
  `-Program of Study` TEXT NOT NULL,
  `-Academic Record` TEXT NOT NULL)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`user preferences`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`user preferences` (
  `-Days` DATE NOT NULL,
  `-Times` TIME NOT NULL,
  `-Number of Courses` INT(11) NOT NULL,
  `-Constraints` TEXT NOT NULL)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`student record`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`student record` (
  `-Completed Courses` VARCHAR(200) DEFAULT NULL,
  `-Academic Requirements` VARCHAR(200)DEFAULT NULL,
  INDEX `fk_student record_student1_idx` (`-Completed Courses` ASC) VISIBLE,
  INDEX `fk_student record_student2_idx` (`-Academic Requirements` ASC) VISIBLE,
  CONSTRAINT `fk_student record_student1`
    FOREIGN KEY (`-Completed Courses`)
    REFERENCES `soen341`.`student` (`-Academic Record`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_student record_student2`
    FOREIGN KEY (`-Academic Requirements`)
    REFERENCES `soen341`.`student` (`-Academic Record`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_student record_user preferences1`
    FOREIGN KEY (`-Completed Courses`)
    REFERENCES `soen341`.`user preferences` (`-Number of Courses`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


