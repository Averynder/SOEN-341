COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`course`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `soen341`.`course` (
  `-Course Title` VARCHAR(200)DEFAULT NULL,
  `-Class Number` INT(11) NULL DEFAULT NULL,
  `-Description` TEXT NULL DEFAULT NULL,
  `-Prerequisites` TEXT NULL DEFAULT NULL,
  `-Co-requisites` TEXT NULL DEFAULT NULL,
  `-Credits` INT(11) NULL DEFAULT NULL,
  `-Term` TEXT NULL DEFAULT NULL,
  `-Sessions` INT(11) NULL DEFAULT NULL,
  `-Capacity` INT(11) NULL DEFAULT NULL,
  `-Waitlist` INT(11) NULL DEFAULT NULL,
  INDEX `fk_course_teacher1_idx` (`-Course Title` ASC) VISIBLE,
  CONSTRAINT `fk_course_teacher1`
    FOREIGN KEY (`-Course Title`)
    REFERENCES `soen341`.`teacher` (`-Course List`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_course_student record1`
    FOREIGN KEY (`-Course Title`)
    REFERENCES `soen341`.`student record` (`-Completed Courses`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `fk_course_course selection1`
    FOREIGN KEY (`-Course Title`)
    REFERENCES `soen341`.`course selection` (`-Course List`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
COLLATE = utf8mb4_0900_ai_ci;


-- -----------------------------------------------------
-- Table `soen341`.`lecture`
-- -----------------------------------------------------
